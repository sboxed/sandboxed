  "version": "0.2.0",
  "configurations": [
    {
      "name": "Generator",
      "cwd": "example/monorepo/ui_book",
      "request": "launch",
      "type": "dart",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "args": ["build", "-d"]
    },
    {
      "name": "material_book",
      "cwd": "example/material_book",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "material_book (profile mode)",
      "cwd": "example/material_book",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "material_book (release mode)",
      "cwd": "example/material_book",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "sandbox",
      "cwd": "apps/sandbox",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "sandbox (profile mode)",
      "cwd": "apps/sandbox",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "sandbox (release mode)",
      "cwd": "apps/sandbox",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
  ]
}